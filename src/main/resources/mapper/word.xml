<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="bit.project.lawbot.mapper.WordMapper">
    <select id="selectWordList" parameterType="WordDTO" resultType="WordDTO" >
        select * from cloud;
    </select>

    <select id="selectWord" parameterType="WordDTO" resultType="WordDTO" >
        select * from cloud where cloud_idx = #{cloud_idx};
    </select>
</mapper>